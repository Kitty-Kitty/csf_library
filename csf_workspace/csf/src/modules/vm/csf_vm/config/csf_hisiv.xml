<?xml version="1.0" encoding="utf-8"?>
<hisiv_configure>
  <!--表示海思平台的系统配置信息-->
  <system>
    <!--表示海思平台的功耗场景类型，取值（1080P_30, 3M_30, 1080P_60, 5M_30）-->
    <profile>1080P_30</profile>
    <!--表示对齐方式-->
    <align_width>16</align_width>
    <!--最大内存池数量-->
    <max_pool_count>128</max_pool_count>
  </system>
</hisiv_configure>

